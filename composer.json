{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">= 8.1",
    "drupal/http_cache_control": "^2.1",
    "drupal/lagoon_logs": "^2.1",
    "drupal/purge": "^3.4",
    "drupal/stage_file_proxy": "^2.0 || ^3.0",
    "drush/drush": "^12"
  },
  "autoload": {
    "psr-4": {
      "IconAgency\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {},
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/drush/drush.yml": "assets/drush.yml",
        "[project-root]/.phpcs.xml.dist": "assets/.phpcs.xml.dist"
      }
    }
  }
}
