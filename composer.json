{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/core": "^10.0"
  },
  "require-dev": {
    "drush/drush": "^11.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "https://www.drupal.org/project/fastly/issues/2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff",
        "https://www.drupal.org/project/fastly/issues/3205426": "https://www.drupal.org/files/issues/2021-03-29/fastly-validate-credentials-3205426-6.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/drush/drush.yml": "drush.yml"
      }
    }
  },
  "suggest": {
    "composer/installers": "^1.12 || ^2.0",
    "amazeeio/drupal_integrations": "^0.3.6",
    "oomphinc/composer-installers-extender": "^2.0"
  }
}
