{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/fastly": "3.14",
    "drupal/http_cache_control": "^2.0",
    "drupal/lagoon_logs": "^1.1.0",
    "drupal/purge": "^3.1",
    "drupal/stage_file_proxy": "^1.1",
    "drush/drush": "^10.6.0"
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "https://www.drupal.org/project/fastly/issues/2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/.vscode/extensions.json": "assets/.vscode/extensions.json",
        "[project-root]/.vscode/settings.json": {
          "mode": "replace",
          "path": "assets/.vscode/settings.json",
          "overwrite": false
        },
        "[project-root]/drush/drush.yml": "assets/drush/drush.yml",
        "[project-root]/.phpcs.xml.dist": "assets/.phpcs.xml.dist",
        "[project-root]/renovate.json": "assets/renovate.json",
        "[project-root]/config/sync/.gitkeep": {
          "mode": "replace",
          "path": "assets/config/sync/.gitkeep",
          "overwrite": false
        },
        "[project-root]/config/dev/README.md": {
          "mode": "replace",
          "path": "assets/config/dev/README.md",
          "overwrite": false
        },
        "[project-root]/assets/README.md": {
          "mmore": "replace",
          "path": "assets/assets/README.md",
          "overwrite": false
        },
        "[project-root]/README.md": {
          "mode": "replace",
          "path": "assets/README.md",
          "overwrite": false
        },
        "[project-root]/.nvmrc": {
          "mode": "replace",
          "path": "assets/.nvmrc",
          "overwrite": false
        }
      }
    }
  },
  "suggest": {
    "amazeeio/drupal_integrations": "Integrate with Lagoon",
    "oomphinc/composer-installers-extender": "Install extra packages such as npm",
    "cweagans/composer-patches": "Patch up files"
  }
}
