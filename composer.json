{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/core": "^10.0",
    "drush/drush": "^11.0",
    "drupal/http_cache_control": "^2.x-dev",
    "drupal/purge": "3.x-dev",
    "drupal/stage_file_proxy": "1.x-dev"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff",
        "3205426": "https://www.drupal.org/files/issues/2021-03-29/fastly-validate-credentials-3205426-6.patch"
      },
      "drupal/http_cache_control": {
        "3283516: D10": "https://git.drupalcode.org/project/http_cache_control/-/merge_requests/3.patch"
      },
      "drupal/purge": {
        "3272193: D10": "https://git.drupalcode.org/project/purge/-/merge_requests/11.patch"
      },
      "drupal/stage_file_proxy": {
        "3283529: D10": "https://git.drupalcode.org/project/stage_file_proxy/-/merge_requests/11.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/drush/drush.yml": "assets/drush.yml"
      }
    }
  },
  "suggest": {
    "composer/installers": "^2.0",
    "amazeeio/drupal_integrations": "^0.3.7",
    "oomphinc/composer-installers-extender": "^2.0"
  }
}
