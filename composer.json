{
  "name": "iconagency/lagoon-drupal",
  "description": "Icon Agency Lagoon Integrations.",
  "type": "metapackage",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/fastly": "3.14",
    "drupal/http_cache_control": "^2.0",
    "drupal/lagoon_logs": "^1.1.0",
    "drupal/purge": "^3.1",
    "drupal/stage_file_proxy": "^1.1"
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "https://www.drupal.org/project/fastly/issues/2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff"
      }
    }
  },
  "suggest": {
    "amazeeio/drupal_integrations": "Integrate with Lagoon",
    "oomphinc/composer-installers-extender": "Install extra packages such as npm",
    "cweagans/composer-patches": "Patch up files"
  }
}
