{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8",
      "exclude": [
        "drupal/http_cache_control",
        "drupal/purge"
      ]
    },
    {
      "type": "package",
      "package": {
        "name": "drupal/http_cache_control",
        "version": "dev-custom",
        "type": "drupal-module",
        "source": {
          "type": "git",
          "url": "git@git.drupal.org:issue/http_cache_control-3283516.git",
          "reference": "3283516-drupal-10-support"
        }
      }
    },
    {
      "type": "git",
      "url": "git@git.drupal.org:issue/purge-3272193.git"
    }
  ],
  "require": {
    "drupal/core": "^10.0",
    "drush/drush": "^11.0",
    "drupal/http_cache_control": "dev-custom#0238e5818fb4113a19437ef94c26c1396fb462a0",
    "drupal/purge": "dev-3272193-update-symfony-response"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {},
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/drush/drush.yml": "assets/drush.yml"
      }
    }
  },
  "suggest": {
    "composer/installers": "^2.0",
    "amazeeio/drupal_integrations": "^0.3.7",
    "oomphinc/composer-installers-extender": "^2.0"
  }
}
