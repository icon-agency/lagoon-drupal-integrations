{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/core-vendor-hardening": "^9.2",
    "drupal/fastly": "3.14",
    "drupal/http_cache_control": "^2.0",
    "drupal/lagoon_logs": "^1.1.0",
    "drupal/purge": "^3.1",
    "drupal/stage_file_proxy": "^1.1",
    "drush/drush": "^10.6.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "https://www.drupal.org/project/fastly/issues/2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/.vscode/extensions.json": "assets/.vscode/extensions.json",
        "[project-root]/.vscode/settings.json":  "assets/.vscode/settings.json",
        "[project-root]/drush/drush.yml": "assets/drush/drush.yml",
        "[project-root]/.phpcs.xml.dist": "assets/.phpcs.xml.dist",
        "[project-root]/README.md": {
          "path": "assets/README.md",
          "overwrite": false
        },
        "[project-root]/.nvmrc": {
          "path": "assets/.nvmrc",
          "overwrite": false
        }
      }
    }
  },
  "suggest": {
    "composer/installers": "^1.12 || ^2.0",
    "amazeeio/drupal_integrations": "^0.3.6",
    "oomphinc/composer-installers-extender": "^2.0"
  }
}
