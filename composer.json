{
  "name": "iconagency/drupal_integrations",
  "description": "Opinionated Icon Agency Lagoon Integrations.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Icon Agency hello@iconagency.com.au",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">= 8.1",
    "drupal/http_cache_control": "2.x-dev",
    "drupal/lagoon_logs": "2.x-dev",
    "drupal/purge": "^3.4",
    "drush/drush": "^11"
  },
  "autoload": {
    "psr-4": {
      "IconAgency\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "patches": {
      "drupal/fastly": {
        "https://www.drupal.org/project/fastly/issues/2908228": "https://git.drupalcode.org/project/fastly/-/merge_requests/1.diff",
        "https://www.drupal.org/project/fastly/issues/3205426": "https://www.drupal.org/files/issues/2021-03-29/fastly-validate-credentials-3205426-6.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/drush/drush.yml": "assets/drush.yml",
        "[project-root]/.phpcs.xml.dist": "assets/.phpcs.xml.dist"
      }
    }
  }
}
